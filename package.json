{
	"name": "jarrodldavis.com",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"packageManager": "pnpm@10.15.0",
	"engines": {
		"node": "22.x",
		"pnpm": "10.15.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "pnpm --sequential /prepare:/",
		"prepare:env": "cp -n .env.example .env",
		"prepare:svelte": "svelte-kit sync || echo ''",
		"prepare:extract": "prepare/index.ts",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.34.0",
		"@ibm/plex-mono": "^1.1.0",
		"@ibm/plex-sans": "^1.1.0",
		"@ibm/plex-serif": "^1.1.0",
		"@sveltejs/adapter-vercel": "^5.10.2",
		"@sveltejs/enhanced-img": "^0.8.1",
		"@sveltejs/kit": "^2.36.2",
		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"@tailwindcss/vite": "^4.1.12",
		"@total-typescript/ts-reset": "^0.6.1",
		"@types/node": "^22.17.2",
		"@types/react": "^19.1.11",
		"@types/react-dom": "^19.1.7",
		"@vercel/analytics": "^1.5.0",
		"@vercel/og": "^0.8.5",
		"@vercel/speed-insights": "^1.2.0",
		"ajv": "^8.17.1",
		"ajv-formats": "^3.0.1",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"ical.js": "^2.2.1",
		"json-schema-to-ts": "^3.1.1",
		"mrmime": "^2.0.1",
		"playwright": "^1.55.0",
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^4.2.0",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"react": "^19.1.1",
		"sass": "^1.90.0",
		"svelte": "^5.38.3",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.12",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.40.0",
		"typescript-svelte-plugin": "^0.3.50",
		"vite": "^7.1.3",
		"yaml": "^2.8.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"@vercel/speed-insights",
			"esbuild",
			"sharp"
		],
		"ignoredBuiltDependencies": [
			"@ibm/plex-mono",
			"@ibm/plex-sans",
			"@ibm/plex-serif"
		]
	}
}
