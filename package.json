{
  "name": "jarrodldavis.com",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.5",
  "engines": {
    "node": "22.x",
    "pnpm": "9.15.5",
    "npm": "0.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "prepare": "./scripts/prepare.mjs",
    "dev": "pnpm /dev:/",
    "dev:typst": "typst watch imprecv/template.typ public/jarrod-davis-resume.pdf",
    "dev:next": "next dev",
    "dev:spotlight": "spotlight",
    "build": "pnpm --sequential /build:/",
    "build:typst": "typst compile imprecv/template.typ public/jarrod-davis-resume.pdf",
    "build:next": "next build",
    "start": "next start",
    "lint": "pnpm /lint:/",
    "lint:next": "next lint",
    "lint:prettier": "prettier --check .",
    "lint:loaders": "tsc -p loaders",
    "lint:scripts": "tsc -p scripts"
  },
  "pnpm": {
    "patchedDependencies": {
      "geist@1.3.1": "patches/geist@1.3.1.patch"
    }
  },
  "dependencies": {
    "@sentry/nextjs": "^8.54.0",
    "@spotlightjs/spotlight": "^2.10.3",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "@vercel/toolbar": "^0.1.30",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "geist": "^1.3.1",
    "next": "15.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-markdown": "^9.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/postcss": "^4.0.4",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/node": "^22.13.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "eslint": "^9.20.0",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "ical.js": "^2.1.0",
    "json-schema-to-typescript": "^15.0.4",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sharp": "^0.33.5",
    "tailwindcss": "^4.0.4",
    "typescript": "^5.7.3",
    "yaml": "^2.7.0"
  }
}
