{
	"name": "jarrodldavis.com",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"packageManager": "pnpm@8.15.4",
	"engines": {
		"node": "20.11.1",
		"pnpm": "8.15.4"
	},
	"scripts": {
		"generate": "pnpm --stream /generate:/",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"format": "pnpm --sequential /format:/",
		"lint": "pnpm --stream /lint:/",
		"test": "pnpm --stream /test:/",
		"generate:svelte": "svelte-kit sync",
		"format:eslint": "eslint --fix . --color",
		"format:stylelint": "stylelint --fix '**/*.{css,svelte}' --color",
		"format:prettier": "prettier --write . --color",
		"lint:config": "tsc -p tsconfig.config.json --pretty",
		"lint:eslint": "eslint . --color",
		"lint:prettier": "prettier --check . --color",
		"lint:stylelint": "stylelint '**/*.{css,svelte}' --color",
		"lint:svelte": "svelte-check --tsconfig ./tsconfig.json -- --color",
		"test:integration": "FORCE_COLOR=1 playwright test",
		"test:unit": "vitest run --color"
	},
	"devDependencies": {
		"@csstools/postcss-global-data": "^2.1.1",
		"@playwright/test": "^1.42.1",
		"@sveltejs/adapter-vercel": "^5.1.1",
		"@sveltejs/enhanced-img": "^0.1.8",
		"@sveltejs/kit": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/eslint": "8.56.5",
		"@types/node": "^20.11.27",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"@vercel/analytics": "^1.2.2",
		"@vercel/speed-insights": "^1.0.10",
		"browserslist-to-esbuild": "^2.1.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-compat": "^4.2.0",
		"eslint-plugin-svelte": "2.36.0-next.10",
		"mrmime": "^2.0.0",
		"open-props": "^1.6.21",
		"playwright": "^1.42.1",
		"postcss-html": "^1.6.0",
		"postcss-jit-props": "^1.0.14",
		"postcss-preset-env": "^9.5.1",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-svelte": "^3.2.2",
		"stylelint": "^16.2.1",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-no-unsupported-browser-features": "^8.0.1",
		"svelte": "5.0.0-next.78",
		"svelte-check": "^3.6.7",
		"tslib": "^2.6.2",
		"typescript": "^5.4.2",
		"typescript-svelte-plugin": "^0.3.37",
		"vite": "^5.1.6",
		"vitest": "^1.3.1",
		"yaml": "^2.4.1",
		"zod": "^3.22.4"
	},
	"pnpm": {
		"overrides": {
			"node-releases": "github:chicoxyzzy/node-releases"
		},
		"patchedDependencies": {
			"postcss-jit-props@1.0.14": "patches/postcss-jit-props@1.0.14.patch"
		}
	}
}
